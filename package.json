{
  "name": "node-gitlab-api",
  "version": "3.0.1",
  "description": "Full NodeJS implementation of the GitLab API. Supports Promises, Async/Await.",
  "main": "dist/latest/index.js",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "build:clean": "rimraf -rf dist && mkdirp -p dist",
    "build:es6": "babel src -d dist/latest --config-file=./.babelrc",
    "build:es5": "babel src -d dist/es5  --config-file=./.babelrc-es5",
    "build": "npm run build:clean && npm run build:es6 && npm run build:es5",
    "lint": "node_modules/eslint/bin/eslint.js ./src ./tests",
    "test:infrastructure": "jest tests/infrastructure; exit 0",
    "test:services": "jest tests/services; exit 0",
    "test": "jest && codecov",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdalrymple/node-gitlab-api"
  },
  "keywords": [
    "gitlab",
    "api",
    "es6",
    "es5",
    "request"
  ],
  "author": "Justin Dalrymple <justin.s.dalrymple@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.44",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0-beta.44",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "jest-tap-reporter": "^1.9.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.44",
    "humps": "^2.0.1",
    "lodash.pick": "^4.4.0",
    "parse-link-header": "^1.0.1",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "url-join": "^4.0.0"
  }
}
